set editing-mode vi

set completion-ignore-case on
set show-all-if-ambiguous on
set show-all-if-unmodified on

set editing-mode vi
set keymap vi-insert

"\C-p":previous-history
"\C-n":next-history
"\C-a":beginning-of-line
"\C-e":end-of-line

"\C-l":clear-screen

"\e[1;5C": forward-word
"\e[1;5D": backward-word
"\e[5C": forward-word
"\e[5D": backward-word
"\e\e[C": forward-word
"\e\e[D": backward-word
